import java.util.Scanner;

public class Task10 {
    public static void main (String [] args){
        Scanner sc = new Scanner (System.in);
        
        System.out.println ("Please enter the number.");
        int num = sc.nextInt ();
        
        int count2 = 0;
        for (int count = 1; count <= num; ++count) {
            if (num%count) {
               ++count2 
            }
        }
    }
}