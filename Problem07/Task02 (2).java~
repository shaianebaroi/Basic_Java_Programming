//FORMALITIES

public class Task01 {
    public static void main (String [] args) {
        
        //DECLARING ARRAY
        int []marks = new int [] {10,30,20,50,40}; 
        
        int max = marks[0];
        int min = marks[0];
        
        //FINDING AVERAGE
        int sum = 0;
        for (int i = 0; i < marks.length; ++i){
            sum = sum + marks[i];
        }
        int average = sum/marks.length;
        
        System.out.println (average);            
    }
}
